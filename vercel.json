{
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ]
}