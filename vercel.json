{
  "version": 2,
  "name": "iit-madras-bot",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/webhook",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "TELEGRAM_TOKEN": ""7673072912:AAE2jkuvfU69hy4Z0nz-qmySf2uXkb5vw1E",
    "GEMINI_API_KEY": "@gemini_api_key", 
    "MONGO_URI": "@mongo_uri"
  }
}